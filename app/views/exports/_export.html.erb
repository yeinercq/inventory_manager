<div class="mt-2">
  <div class="card shadow-sm">
    <div class="card-body">
      <div class="container">
        <div class="row align-items-center">
          <div class="col">
            <%= export.key %>
          </div>
          <div class="col">
            <%= export.created_at %>
          </div>
          <div class="col">
            <%= t('common.status') %>: <%= export.status %>
          </div>
          <div class="col">
            <%= t('common.file') %>: <%= link_to t('common.download'), export.file_path %>
          </div>
        </div>

      </div>
    </div>
  </div>
</div>
